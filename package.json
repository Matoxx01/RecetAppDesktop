{
  "name": "recetappdesktop",
  "version": "1.0.0",
  "description": "RecetApp Desktop Application",
  "homepage": "https://github.com/Matoxx01/RecetApp-Desktop#readme",
  "bugs": {
    "url": "https://github.com/Matoxx01/RecetApp-Desktop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Matoxx01/RecetApp-Desktop.git"
  },
  "license": "ISC",
  "author": "RecetApps",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^34.3.0",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.recetapp.nombre",
    "productName": "RecetApp",
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "assets/icon.ico",
      "target": "nsis"
    },
    "mac": {
      "icon": "assets/icon.png",
      "target": "dmg"
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": "AppImage"
    }
  }
}